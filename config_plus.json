{
  "dataset_path": "/data/ymx/dataset/imagenet-part",
  "target_condition": "depth",
  "tag": "default",
  "source_conditions": [
    "canny",
    "sketch",
    "color"
  ],

  "depth_w": 1.0,
  "canny_w": 1.1,
  "sketch_w": 0.80,
  "color_w": 1.1,

  "hed_w": 1.05,
  "lineart_w": 0.9,
  "illusion_w": 1.05,

  "img_size": 128,
  "batch_size": 100,
  "num_workers": 8,
  "epochs": 600,
  "lr": 2e-4,
  "beta": 0.01,
  "beta_warmup_epochs": 60,
  "seed": 42,
  "device": "cuda",
  "output_dir": "./output_plus",
  "save_interval": 10,
  "sample_interval": 5,
  "recon_loss_type": "l1",
  "output_channels": 1,
  "base_channels": 24,
  "depth": 6,
  "attention_type": "cbam",
  "accumulate_grad_batches": 1,
  "gradient_clip_val": 4.0,
  "precision": 32,
  "strategy": "ddp_find_unused_parameters_true",
  "sync_batchnorm": true,
  "device_ids": [
    5,
    6,
    7
  ]
}