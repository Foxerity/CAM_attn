  "input_channels": 3,
  "output_channels": 1,
  "base_channels": 32,
  "depth": 4,
  "attention_type": "cbam",
  "accumulate_grad_batches": 1,
  "gradient_clip_val": 1.0,
  "precision": 16,
  "strategy": "ddp_find_unused_parameters_true",
